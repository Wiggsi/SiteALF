<form method="post" action="{{ path('gd_cog_delete', {'id': appelCOG.id}) }}"
      onsubmit="return confirm('Confirmer la suppression de l\'appel :');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ appelCOG.id) }}">
    <button class="btn btn-outline-danger">
        <span class="oi oi-delete"></span>
    </button>
</form>