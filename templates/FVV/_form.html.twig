{{ form_start(form) }}
<div class="row">
    <div class="col-md-5">
        {{ form_errors(form) }}
        {{ form_row(form.date) }}
        {{ form_row(form.propName) }}
        {{ form_row(form.propFirstName) }}
        {{ form_row(form.tel) }}
        {{ form_row(form.type) }}
        {{ form_row(form.plaque) }}
        {{ form_row(form.save) }}
    </div>
    <div class="col-md-7" id="colContent">
        {{ form_row(form.comment) }}
    </div>
</div>
{{ form_end(form) }}
